# PharmacoBench Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# HuggingFace Token (for deployment)
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here

# Data cache directory (optional, defaults to data/cache/)
DATA_CACHE_DIR=data/cache

# Model checkpoints directory (optional, defaults to models/checkpoints/)
MODEL_CHECKPOINT_DIR=models/checkpoints

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
